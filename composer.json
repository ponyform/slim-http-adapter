{
    "name": "ponyform/slim-http-adapter",
    "description": "PonyForm HTTP adapter to integrate with slim applications",
    "version": "v0.0.0",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Andreas Mischke",
            "email": "andreas@mischke.me"
        }
    ],
    "require": {
        "ponyform/core": "^0.0",
        "slim/slim": "^4.10"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/*"
        },
        {
            "type": "path",
            "url": "../../plugins/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "PonyForm\\SlimHttpAdapter\\": ""
        }
    }
}